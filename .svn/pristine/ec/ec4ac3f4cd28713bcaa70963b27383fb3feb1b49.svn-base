html,body,.mui-content{
	height: 100%;
	background: #fff;
}
header {
	width: 100%;
	background-color: #FA625F !important;
	box-shadow: none !important;
}

.mui-content {
	background: #fff;
}

header a {
	color: #fff;
}

.mui-title {
	color: #fff!important;
}

.head_t{
	height: 45px;
	line-height: 45px;
	font-size: 14px;
	padding-left: 15px;
	border-bottom: 5px solid #f2f2f2;
}

.head_t img{
	width: 5%;
	vertical-align: middle;
	margin-bottom: 2px;
}
.mui-table-view:before,
.mui-table-view:after,
.mui-table-view-cell:after {
	height: 0;
}

.mui-table-view li {
	height: 85px;
	border-bottom: 1px solid #eee;
}

.mui-table-view li .img {
	width: 15%;
	margin-top: 5px;
}

.mui-table-view li .img img {
	width: 100%;
}

.mui-table-view li .info {
	width: 50%;
	padding-left: 10px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	margin-top: 5px;
}

.mui-table-view li .info p {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.mui-table-view li .info .name {
	font-size: 14px;
	color: #000;
	margin-bottom: 10px;
}

.mui-table-view li .info .remark {
	font-size: 12px;
}

.mui-table-view li .right {
	width: 32%;
	font-size: 14px;
	color: #8F8F94;
	padding-right: 15px;
	text-align: right;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	margin-top: 5px;
}
.mui-table-view li .right p{
	margin-bottom: 10px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.mui-table-view li .right p:first-child {
	font-size: 16px;
	color: #F99F3E;
}

.mui-table-view-cell>a:not(.mui-btn) {
	height: 85px;
}


/*详情页*/

.head {
	height: 120px;
	text-align: center;
	padding-top: 20px;
	background: -webkit-linear-gradient(#FFAF5C, #FF9149);
	background: -o-linear-gradient(#FFAF5C, #FF9149);
	background: -moz-linear-gradient(#FFAF5C, #FF9149);
	background: linear-gradient(#FFAF5C, #FF9149);
}
.head p{
	color: #BCF8FF;
}
.head .money{
	color: #fff;
	font-size: 26px;
	padding-top: 10px;
}
