.condition{
	margin-top: 60px;
	padding: 0 15px;
}
.condition h1{
	font-size: 15px;
	margin-bottom: 10px;
}
.condition p{
	margin-bottom: 5px;
}
.condition ul li{
	position: relative;
	width: calc(100% / 4);
	text-align: center;
	float: left;
	margin-top: 15px;
}
.condition ul li p{
	font-size: 12px;
}
.condition ul li img{
	width: 26px;
	vertical-align: bottom;
}
.condition ul li span{
	position: absolute;
	top: 8px;
	right: -6px;
	color: #ccc;
	font-size: 12px !important;
}
.show{
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	padding: 10px 15px;
	background: #0075FF;
	border: 1px solid #0075FF;
	border-radius: 0;
	font-size: 15px;
}

.popup{
	display: none;
	position: fixed;
	top: 20%;
	left: 50%;
	z-index: 99;
	width: 90%;
	margin-left: -45%;
	background: #fff;
	border-radius: 8px;
}
.popup .icon-cuo{
	position: absolute;
	top:5px;
	right: 10px;
	color: #fff;
	font-size: 20px !important;
}
.popup .title{
	width: 100%;
	height: 80px;
	text-align: center;
	padding-top: 15px;
	background: url(../img/dai_p_bg.png) no-repeat;
	background-size: 100% 100%;
}
.popup .title p{
	color: #fff;
}
.popup .title p:last-child{
	font-size: 12px;
}
.popup .content{
	padding: 15px;
}
.popup .mui-input-row{
	font-size: 14px;
	border-bottom: 1px solid #eee;
}
.popup .mui-input-row input{
	font-size: 14px;
}
.apply{
	left: 50%;
	width: 100%;
	margin-left: -50%;
	margin-top: 20px;
	margin-bottom: 10px;
	padding: 10px 15px;
	background: #0C7DFF;
	border: 1px solid #0C7DFF;
}
.mengban{
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.5);
	z-index: 98;
}
